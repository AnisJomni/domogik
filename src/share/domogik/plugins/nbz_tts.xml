<package type="plugin">
  <name>nbz_tts</name>
  <technology>communication</technology>
  <description>Send message to Nabaztag</description>
  <detail>Nabaztag text_to_speech management is an external service provide by Mindscape</detail>
  <version>0.1</version>
  <documentation>http://wiki.domogik.org</documentation>
  <author>Kriss</author>
  <author-email>Kriss</author-email>
  <domogik-min-version>0.1.0</domogik-min-version>
  <configuration-keys>
    <key>
      <order-id>0</order-id>
      <name>startup-plugin</name>
      <type>boolean</type>
      <description>Automatically start plugin at Domogik startup</description>
      <default-value>False</default-value>
    </key>
    <key>
      <order-id>1</order-id>
      <name>serial</name>
      <type>string</type>
      <description>Serial of your Nabaztag to control it whith Mindscape's API</description>
      <default-value>0000000000</default-value>
    </key>
    <key>
      <order-id>2</order-id>
      <name>token</name>
      <type>string</type>
      <description>Token of your Nabaztag to control it whith Mindscape's API</description>
      <default-value>0000000000</default-value>
    </key>
    <key>
      <order-id>3</order-id>
      <name>voice</name>
