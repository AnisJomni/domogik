{
    "configuration": [
        {
            "default": "False", 
            "desc": "Automatically start plugin at Domogik startup", 
            "id": "0", 
            "interface": "no", 
            "key": "startup-plugin", 
            "optionnal": "no", 
            "options": [], 
            "type": "boolean"
        }, 
        {
            "default": "/dev/modem", 
            "desc": "Modem device (ex: /dev/ttyUSB0 for an usb modem)", 
            "id": "1", 
            "interface": "no", 
            "key": "device", 
            "optionnal": "no", 
            "options": [], 
            "type": "string"
        }, 
        {
            "default": "AT#CID=1", 
            "desc": "AT CID command to be used by plugin to activate Caller Id mode on modem.", 
            "id": "2", 
            "interface": "no", 
            "key": "cid-command", 
            "optionnal": "no", 
            "options": [], 
            "type": "string"
        }
    ], 
    "dependencies": [
        {
            "id": "pyserial (>=2.5)", 
            "type": "python"
        }
    ], 
    "device_feature_models": [
        {
            "device_type_id": "communication.caller_id", 
            "feature_type": "sensor", 
            "id": "communication.caller_id.number", 
            "name": "Number", 
            "parameters": "{}", 
            "return_confirmation": "0", 
            "stat_key": "phone", 
            "value_type": "string"
        }
    ], 
    "device_types": [], 
    "files": [
        "src/share/domogik/design/plugin/cidmodem/icon.png", 
        "src/share/domogik/plugins/cidmodem.xml", 
        "src/share/domogik/stats/communication/cid.basic-cidmodem.xml", 
        "src/domogik/xpl/bin/cidmodem.py", 
        "src/domogik/xpl/lib/cidmodem.py"
    ], 
    "identity": {
        "author": "Fritz", 
        "author_email": "fritz.smh at gmail.com", 
        "category": "communication", 
        "changelog": "0.2\n- adapt plugin for release 0.2.0\n\n0.1\n- plugin creation", 
        "description": "Get caller id with a modem : when receiving an incoming call on a classic telephonic line, this plugin gets the incoming call number and send it on xPL.", 
        "documentation": "http://wiki.domogik.org/plugin_cidmodem", 
        "domogik_min_version": "0.2.0", 
        "fullname": "plugin-cidmodem", 
        "id": "cidmodem", 
        "name": "cidmodem", 
        "type": "plugin", 
        "version": "0.2a0"
    }, 
    "json_version": 1, 
    "technology": {
        "description": "Telephony, videophone, mails, messaging", 
        "id": "communication", 
        "name": "Communication"
    }, 
    "udev-rules": [
        {
            "desc": "Example for a common modem", 
            "filename": "modem.rules", 
            "model": "modem", 
            "rule": "UBSYSTEMS==\"usb\", ATTRS{idVendor}==\"067b\", ATTRS{idProduct}==\"2303\", SYMLINK+=\"modem\", MODE=\"0666\""
        }
    ]
