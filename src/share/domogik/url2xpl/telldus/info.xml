<technology id="telldus">
  <command name="info">
    <schema>helper.basic</schema>
    <command-key>command</command-key>
    <address-key>plugin</address-key>
    <command-xpl-value>info</command-xpl-value>
    <parameters>
        <parameter key="device" location="1"/>
    </parameters>
  </command>
  <listener>
    <schema>helper.basic</schema>
    <xpltype>xpl-trig</xpltype>
    <filter>
      <key name="status" value="ok" />
      <key name="plugin" value="@address@"/>
    </filter>
  </listener>
</technology>
