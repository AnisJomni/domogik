        "fullname": "plugin-plcbus", 
        "id": "plcbus", 
        "name": "plcbus", 
        "type": "plugin", 
        "version": "0.2a0"
    }, 
    "json_version": 1, 
    "technology": {
        "description": "PLCBus", 
        "id": "plcbus", 
        "name": "PLCBus"
    }, 
    "udev-rules": [
        {
            "desc": "Plcbus 1141U Usb adaptator", 
            "filename": "plcbus.rules", 
            "model": "Plcbus 1141U", 
            "rule": "SUBSYSTEMS==\"usb\", ATTRS{idVendor}==\"067b\", ATTRS{idProduct}==\"2303\", SYMLINK+=\"plcbus\", MODE=\"0666\""
        }
    ]
