<plugin>
  <name>wol_ping</name>
  <technology>computer</technology>
  <description>Wake on lan</description>
  <version>0.1</version>
  <documentation>http://wiki.domogik.org/tiki-index.php?page=plugins/Wol</documentation>
  <configuration-keys>
    <key>
      <order-id>0</order-id>
      <name>startup-plugin</name>
      <type>boolean</type>
      <description>Automatically start plugin at Domogik startup</description>
      <default-value>False</default-value>
    </key>
    <key>
      <order-id>1</order-id>
      <name>cmp-1-name</name>
      <type>string</type>
      <description>Computer 1 name</description>
      <default-value></default-value>
    </key>
    <key>
      <order-id>2</order-id>
      <name>cmp-1-ip</name>
      <type>string</type>
      <description>Computer 1 ip or dns</description>
      <default-value></default-value>
    </key>
    <key>
      <order-id>3</order-id>
      <name>cmp-1-mac</name>
      <type>string</type>
      <description>Computer 1 mac address</description>
      <default-value></default-value>
    </key>
    <key>
      <order-id>4</order-id>
      <name>cmp-1-mac-port</name>
      <type>string</type>
      <description>Computer 1 port used for WOL</description>
      <default-value></default-value>
    </key>
    <key>
      <order-id>5</order-id>
      <name>cmp-2-name</name>
      <type>string</type>
      <description>Computer 2 name</description>
      <default-value></default-value>
    </key>
    <key>
      <order-id>6</order-id>
      <name>cmp-2-ip</name>
      <type>string</type>
      <description>Computer 2 ip or dns</description>
      <default-value></default-value>
    </key>
    <key>
      <order-id>7</order-id>
      <name>cmp-2-mac</name>
      <type>string</type>
      <description>Computer 2 mac address</description>
      <default-value></default-value>
    </key>
    <key>
      <order-id>8</order-id>
      <name>cmp-2-mac-port</name>
      <type>string</type>
      <description>Computer 2 port used for WOL</description>
      <default-value></default-value>
    </key>
  </configuration-keys>
</plugin>
