<package type="plugin">
  <name>zibase</name>
  <technology>zibase</technology>
  <description>ZiBase support</description>
  <detail>ZiBase support</detail>
  <version>0.1.0</version>
  <documentation>http://wiki.domogik.org/plugin_zibase</documentation>
  <author>Cedric BOLLINI</author>
  <author-email>cb.dev@sfr.fr</author-email>
  <configuration-keys>
    <key>
      <order-id>0</order-id>
      <name>startup-plugin</name>
      <type>boolean</type>
      <description>Automatically start plugin at Domogik startup</description>
      <default-value>False</default-value>
    </key>
    <key>
      <order-id>1</order-id>
      <name>ip</name>
      <type>string</type>
      <description>ZiBase IP</description>
      <default-value></default-value>
    </key>
    <key>
      <order-id>2</order-id>
      <name>interface</name>
      <type>string</type>
      <description>Network Interface</description>
      <default-value>eth0</default-value>
    </key>
    <key>
      <order-id>3</order-id>
      <name>port</name>
      <type>int</type>
      <description>Listening Port</description>
      <default-value>17100</default-value>
    </key>
    <key>
      <order-id>4</order-id>
      <name>interv</name>
      <type>int</type>
      <description>Interval between each read internal variables</description>
      <default-value>1000</default-value>
    </key>
  </configuration-keys>
  <files>
    <file path="src/share/domogik/plugins/zibase.xml"/>
    <file path="src/share/domogik/stats/zibase/zibase.basic-zibase.xml"/>
    <file path="src/share/domogik/url2xpl/zibase/preset-dim.xml"/>
    <file path="src/share/domogik/url2xpl/zibase/on.xml"/>
    <file path="src/share/domogik/url2xpl/zibase/off.xml"/>
    <file path="src/domogik/xpl/bin/zibase.py"/>
    <file path="src/domogik/xpl/lib/zibase.py"/>
  </files>
  <dependencies/>
