<package type="plugin">
  <id>zalarms</id>
  <technology>cron</technology>
  <description>
  A poor interface to the cron plugin.
  You can only create an alarm message.
  Read the wiki for documentation.
  </description>
  <changelog>
0.2
- #TO DEFINE#
0.1
- Initial release</changelog>
  <version>0.2a0</version>
  <documentation>http://wiki.domogik.org/plugin_zalarms</documentation>
  <author>Sebastien GALLET</author>
  <author-email>sgallet at gmail dot com</author-email>
  <domogik-min-version>0.2.0</domogik-min-version>
  <configuration-keys>
    <key>
      <order-id>0</order-id>
      <name>startup-plugin</name>
      <type>boolean</type>
      <description>Automatically start plugin at Domogik startup</description>
      <default-value>False</default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>1</order-id>
      <name>device</name>
      <type>string</type>
      <description>The name of the alarm</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>2</order-id>
      <name>alarmtype</name>
      <type>string</type>
      <description>alarm or dawnalarm</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>3</order-id>
      <name>alarm1</name>
      <type>string</type>
      <description>A string like MoTuWeThFr,09:30</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>4</order-id>
      <name>alarm2</name>
      <type>string</type>
      <description>Or like MoTuWeThFrSaSu,07:00-08:00,17:00-21:00</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>5</order-id>
      <name>alarm3</name>
      <type>string</type>
      <description>Or for a dawnalarm like SaSu,08:00-09:00</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>6</order-id>
      <name>nstschema</name>
      <type>string</type>
      <description>The schema of the message to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>7</order-id>
      <name>nsttype</name>
      <type>string</type>
      <description>The type of the message to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>8</order-id>
      <name>parameter0</name>
      <type>string</type>
      <description>The name of the parameter 0 to send (ie command)</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>9</order-id>
      <name>valueon0</name>
      <type>string</type>
      <description>The on value of the parameter 0 to send (ie on)</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>10</order-id>
      <name>valueoff0</name>
      <type>string</type>
      <description>The off value of the parameter 0 to send (ie off)</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>11</order-id>
      <name>parameter1</name>
      <type>string</type>
      <description>The name of the parameter 1 to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>12</order-id>
      <name>valueon1</name>
      <type>string</type>
      <description>The on value of the parameter 1 to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>13</order-id>
      <name>valueoff1</name>
      <type>string</type>
      <description>The off value of the parameter 1 to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>14</order-id>
      <name>nstfield1</name>
      <type>string</type>
      <description>The nested field 1 to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>15</order-id>
      <name>nstvalue1</name>
      <type>string</type>
      <description>The value of the nested field to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>16</order-id>
      <name>nstfield2</name>
      <type>string</type>
      <description>The nested the field 2 to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>17</order-id>
      <name>nstvalue2</name>
      <type>string</type>
      <description>The value of the nested field to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>18</order-id>
      <name>nstfield3</name>
      <type>string</type>
      <description>The nested the field 3 to send</description>
      <default-value></default-value>
    </key>
    <key>
      <interface>yes</interface>
      <order-id>19</order-id>
      <name>nstvalue3</name>
      <type>string</type>
      <description>The value of the nested field to send</description>
      <default-value></default-value>
    </key>
  </configuration-keys>
  <files>
    <file path="src/share/domogik/design/plugin/zalarms/icon.png"/>
    <file path="src/share/domogik/plugins/zalarms.xml"/>
    <file path="src/domogik/xpl/bin/zalarms.py"/>
    <file path="src/domogik/xpl/lib/zalarms.py"/>
  </files>
  <dependencies/>
  <udev-rules/>
  <device_technology>
      <id>cron</id>
      <name>Cron</name>
      <description>Cron</description>
  </device_technology>
  <device_types/>
  <device_feature_models/>
</package>
