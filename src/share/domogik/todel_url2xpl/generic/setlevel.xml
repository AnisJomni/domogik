<technology id="generic">
  <command name="setlevel">
    <schema>lighting.basic</schema>
    <address-key>device</address-key>
    <parameters>
      <parameter key="level" location="1" />
      <parameter key="command" value="goto"/>
    </parameters>
  </command>
  <listener>
    <schema>lighting.device</schema>
    <xpltype>xpl-trig</xpltype>
    <filter>
       <key name="device" value="@address@"/>
    </filter>
  </listener>
</technology>
