<package type="plugin">
  <id>xpl_time</id>
  <technology>service</technology>
  <description>This plugin sends date and time on the xPL network every minute</description>
  <changelog>
0.1
- plugin creation
  </changelog>
  <version>0.1a0</version>
  <documentation>http://wiki.domogik.org/plugin_xpl_time</documentation>
  <author>Maxence Dunnewind</author>
  <author-email>maxence at dunnewind.net</author-email>
  <domogik-min-version>0.2.0</domogik-min-version>
  <configuration-keys>
    <key>
      <order-id>0</order-id>
      <name>startup-plugin</name>
      <type>boolean</type>
      <description>Automatically start plugin at Domogik startup</description>
      <default-value>False</default-value>
    </key>
  </configuration-keys>
  <files>
    <file path="src/share/domogik/design/plugin/xpl_time/icon.png"/>
    <file path="src/share/domogik/plugins/xpl_time.xml"/>
    <file path="src/domogik/xpl/bin/xpl_time.py"/>
  </files>
  <dependencies/>
  <udev-rules/>
  <device_technology>
      <id>domogik</id>
      <name>Domogik</name>
      <description>Domogik</description>
  </device_technology>
  <device_types/>
  <device_feature_models/>
</package>
