{
    "configuration": [
        {
            "default": "False", 
            "desc": "Automatically start plugin at Domogik startup", 
            "id": "0", 
            "interface": "no", 
            "key": "startup-plugin", 
            "optionnal": "no", 
            "options": [], 
            "type": "boolean"
        }, 
        {
            "default": "/dev/plcbus", 
            "desc": "Plcbus device", 
            "id": "1", 
            "interface": "no", 
            "key": "device", 
            "optionnal": "no", 
            "options": [], 
            "type": "string"
        }, 
        {
            "default": "FF", 
            "desc": "Usercode value (between 00 and FF), defaults to FF", 
            "id": "2", 
            "interface": "no", 
            "key": "usercode", 
            "optionnal": "no", 
            "options": [], 
            "type": "string"
        }, 
        {
            "default": "30", 
            "desc": "Interval between 2 probes to discover devices and check their state (0 = don't do probing).", 
            "id": "3", 
            "interface": "no", 
            "key": "probe-interval", 
            "optionnal": "no", 
            "options": [], 
            "type": "string"
        }, 
        {
            "default": "ABCDEFGHIJKLMNOP", 
            "desc": "List of housecode to probe. If you know you don't use an house code remove it from the list.", 
            "id": "4", 
            "interface": "no", 
            "key": "probe-list", 
            "optionnal": "no", 
            "options": [], 
            "type": "string"
        }
    ], 
    "dependencies": [
        {
            "id": "pyserial (>=2.5)", 
            "type": "python"
        }
    ], 
    "device_feature_models": [
        {
            "device_type_id": "plcbus.switch", 
            "feature_type": "actuator", 
            "id": "plcbus.switch.switch", 
            "name": "Switch", 
            "parameters": "{&quot;command&quot;:&quot;&quot;,&quot;value0&quot;:&quot;off&quot;, &quot;value1&quot;:&quot;on&quot;}", 
            "return_confirmation": "1", 
            "stat_key": "command", 
            "value_type": "binary"
        }, 
        {
            "device_type_id": "plcbus.dimmer", 
            "feature_type": "actuator", 
            "id": "plcbus.dimmer.switch", 
            "name": "Dimmer", 
            "parameters": "{&quot;command&quot;:&quot;&quot;,&quot;value0&quot;:&quot;off&quot;, &quot;value1&quot;:&quot;on&quot;}", 
            "return_confirmation": "1", 
            "stat_key": "command", 
            "value_type": "binary"
        }, 
        {
            "device_type_id": "plcbus.dimmer", 
            "feature_type": "actuator", 
            "id": "plcbus.dimmer.change", 
            "name": "Change", 
            "parameters": "{&quot;command&quot;:&quot;preset_dim&quot;,&quot;valueMin&quot;:0, &quot;valueMax&quot;:100}", 
            "return_confirmation": "1", 
            "stat_key": "level", 
            "value_type": "range"
        }
    ], 
    "device_types": [], 
    "files": [
        "src/share/domogik/design/plugin/plcbus/icon.png", 
        "src/share/domogik/plugins/plcbus.xml", 
