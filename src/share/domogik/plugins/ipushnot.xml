<package type="plugin">
  <id>ipushnot</id>
  <technology>notification</technology>
  <description>Iphone/Ipad notification system base on external service provide by pushme.to</description>
  <changelog>
0.1.0
- plugin creation
  </changelog>
  <version>0.1.0</version>
  <documentation>http://wiki.domogik.org/plugin_ipushnot</documentation>
  <author>Kriss</author>
  <author-email>Kriss@domogik.org</author-email>
  <domogik-min-version>0.1.0</domogik-min-version>
  <configuration-keys>
    <key>
      <order-id>0</order-id>
      <name>startup-plugin</name>
      <type>boolean</type>
      <description>Automatically start plugin at Domogik startup</description>
      <default-value>False</default-value>
    </key>
  </configuration-keys>
  <files>
    <file path="src/share/domogik/plugins/ipushnot.xml"/>
    <file path="src/domogik/xpl/bin/ipushnot.py"/>
    <file path="src/domogik/xpl/lib/ipushnot.py"/>
  </files>
  <dependencies/>
  <device_technology>
      <id>notification</id>
      <name>Notification</name>
      <description>Notification to a smartphone</description>
  </device_technology>
  <device_types/>
  <device_feature_models/>
</package>

