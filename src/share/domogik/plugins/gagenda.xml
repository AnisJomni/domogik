<package type="plugin">
  <name>gagenda</name>
  <technology>communication</technology>
  <description>Get events from a Google agenda</description>
  <detail>Connect to a google agenda to get events</detail>
  <version>0.1</version>
  <documentation>http://wiki.domogik.org/tiki-index.php?page=plugin_gagenda</documentation>
  <author>Fritz</author>
  <author-email>fritz.smh at gmail.com</author-email>
  <configuration-keys>
    <key>
      <order-id>0</order-id>
      <name>startup-plugin</name>
      <type>boolean</type>
      <description>Automatically start plugin at Domogik startup</description>
      <default-value>False</default-value>
    </key>
    <key>
      <order-id>1</order-id>
      <name>email</name>
      <type>string</type>
      <description>Google email account</description>
      <default-value>john.doc@gmail.com</default-value>
    </key>
    <key>
      <order-id>2</order-id>
      <name>password</name>
      <type>string</type>
      <description>Password for email account</description>
      <default-value>password</default-value>
    </key>
    <key>
      <order-id>3</order-id>
      <name>calendarname</name>
      <type>string</type>
      <description>Calendar name (default : your email)</description>
      <default-value>john.doe@gmail.com</default-value>
    </key>
  </configuration-keys>
  <files/>
  <depandancies/>
</package>
