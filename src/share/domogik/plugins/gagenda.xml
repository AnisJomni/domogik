<package type="plugin">
  <id>gagenda</id>
  <technology>communication</technology>
  <description>Connect to a google agenda to get events agenda</description>
  <changelog>
0.1.0
- plugin creation
  </changelog>
  <version>0.1</version>
  <documentation>http://wiki.domogik.org/plugin_gagenda</documentation>
  <author>Fritz</author>
  <author-email>fritz.smh at gmail.com</author-email>
  <domogik-min-version>0.1.0</domogik-min-version>
  <configuration-keys>
    <key>
      <order-id>0</order-id>
      <name>startup-plugin</name>
      <type>boolean</type>
      <description>Automatically start plugin at Domogik startup</description>
      <default-value>False</default-value>
    </key>
    <key>
      <order-id>1</order-id>
      <name>email</name>
      <type>string</type>
      <description>Google email account</description>
      <default-value>john.doc@gmail.com</default-value>
    </key>
    <key>
      <order-id>2</order-id>
      <name>password</name>
      <type>password</type>
      <description>Password for email account</description>
      <default-value>password</default-value>
    </key>
    <key>
      <order-id>3</order-id>
      <name>calendarname</name>
      <type>string</type>
      <description>Calendar name (default : your email)</description>
      <default-value>john.doe@gmail.com</default-value>
    </key>
  </configuration-keys>
  <files/>
  <dependencies/>
  <device_technology>
      <id>communication</id>
      <name>Communication</name>
      <description>Messaging</description>
  </device_technology>
  <device_types/>
  <device_types/>
  <device_feature_models/>
</package>
